{
	"schemaVersion": 1,
	"id": "theancients",
	"version": "${version}",
	"name": "The Ancients",
	"description": "The Ancient Cities are getting boring? Well this mod aims to add a new dimension, accessed through the portal located in Ancient Cities.\nExplore a beautiful dimension with new structures, mobs and bosses!\nShaders are recommended!",
	"authors": [
		"Buzau"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/theancients/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"buzau.theancients.TheAncients"
		],
		"client": [
			"buzau.theancients.TheAncientsClient"
		],
		"fabric-datagen": [
			"buzau.theancients.TheAncientsDataGenerator"
		]
	},
	"mixins": [
		"theancients.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.7",
		"minecraft": "~1.20.4",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}